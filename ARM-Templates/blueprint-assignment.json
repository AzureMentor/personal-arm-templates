{
    "$schema": "https://schema.management.azure.com/schemas/2018-05-01/subscriptionDeploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "blueprintName": {
            "type": "string"
        },
        "blueprintVersion": {
            "type": "string"
        },
        "blueprintParameters": {
            "type": "array"
        },
        "subscriptionId": {
            "type": "string"
        }
    },
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.Blueprint/blueprintAssignments",
            "name": "Assignment-MyER",
            "apiVersion": "2017-11-11-preview",
            "identity": {
                "type": "systemAssigned"
            },
            "location": "westus2",
            "properties": {
                "blueprintId": "[concat('/providers/Microsoft.Management/managementGroups/ContosoRoot/providers/Microsoft.Blueprint/blueprints/', parameters('blueprintName'), '/versions/', parameters('blueprintVersion'))]",
                "parameters": {
                    "MyER_nsgName": {
                        "value": "MSIT-NSG"
                    },
                    "MyER_otherSubnetPrefix": {
                        "value": "10.69.145.16/28"
                    },
                    "MyER_addressPrefixes": {
                        "value": [
                        "10.69.145.16/28",
                        "10.69.159.176/28"
                        ]
                    },
                    "MyER_gatewayAddressPrefix": {
                        "value": "10.69.159.176/28"
                    },
                    "MyER_dnsServers": {
                        "value": [
                        "10.50.50.50",
                        "10.50.10.50"
                        ]
                    },
                    "MyER_requestId": {
                        "value": "12345"
                    },
                    "MyER_gatewaySubnetName": {
                        "value": "GatewaySubnet"
                    },
                    "MyER_pipName": {
                        "value": "MY-PIP"
                    },
                    "MyER_subnetName": {
                        "value": "MY-SUBNET"
                    },
                    "MyER_vnetName": {
                        "value": "MY-VNET"
                    },
                    "MyER_expressRouteName": {
                        "value": "express-route-across-subs"
                    }
                },
                "resourceGroups": {
                    "MyER": {
                        "name": "My-ER-TEST03"
                    }
                },
                "scope": "[concat('/subscriptions/', subscription().id)]",
                "locks": {
                    "mode": "none"
                }
            }
        }
    ]
}